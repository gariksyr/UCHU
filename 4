#include <iostream>
using namespace std;

class DogCollar{
public:
	void run() { cout << "dog walk with collar" << endl; } //декомпозиция
};
class Dog {
public:
	void run() { feet.run(); }
private: //агрегация
	class Feet {
	public:
		void run() { cout << "dog is running" << endl; }
	};
	Feet feet;
	DogCollar dogCollar;
};
int main() {
	Dog dog;
	dog.run();	
	return 0;
}
